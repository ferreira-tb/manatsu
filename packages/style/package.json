{
  "name": "@manatsu/style",
  "version": "0.6.22",
  "description": "CSS utilities",
  "license": "MIT",
  "private": false,
  "packageManager": "pnpm@8.15.5",
  "homepage": "https://github.com/tsukilabs/manatsu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsukilabs/manatsu.git"
  },
  "bugs": {
    "url": "https://github.com/tsukilabs/manatsu/issues"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "manatsu",
    "scss",
    "style"
  ],
  "type": "module",
  "files": [
    "dist",
    "src/mixins",
    "src/themes"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*",
    "./themes/mana": "./src/themes/mana/index.scss",
    "./mixins/flex": "./src/mixins/flex.scss",
    "./mixins/shadow": "./src/mixins/shadow.scss",
    "./mixins/transition": "./src/mixins/transition.scss",
    "./mixins/utils": "./src/mixins/utils.scss"
  },
  "scripts": {
    "build": "pnpm run --bail \"/^(type-check|build-only)$/\"",
    "build-only": "vite build",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "sass": "^1.72.0"
  },
  "engines": {
    "node": ">=20"
  }
}
