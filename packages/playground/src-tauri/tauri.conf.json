{
  "productName": "playground",
  "version": "0.4.1",
  "identifier": "tsukilabs.manatsu",
  "build": {
    "beforeBuildCommand": "pnpm run build",
    "beforeDevCommand": "pnpm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "main",
        "title": "Playground",
        "height": 600,
        "width": 800,
        "fullscreen": false,
        "resizable": true,
        "visible": false
      }
    ]
  },
  "bundle": {
    "active": false,
    "icon": ["icons/icon.ico"]
  },
  "plugins": {
    "updater": {}
  }
}
