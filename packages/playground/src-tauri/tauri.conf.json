{
  "package": {
    "productName": "playground",
    "version": "0.4.30"
  },
  "build": {
    "beforeBuildCommand": "pnpm run build",
    "beforeDevCommand": "pnpm run dev",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": false,
      "identifier": "tsukilabs.manatsu",
      "icon": ["icons/icon.ico"]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "title": "Playground",
        "width": 800,
        "height": 600,
        "visible": false,
        "fullscreen": false,
        "resizable": true
      }
    ]
  }
}
