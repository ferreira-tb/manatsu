{
  "productName": "playground",
  "version": "0.6.0",
  "identifier": "tsukilabs.manatsu",
  "build": {
    "beforeBuildCommand": "pnpm run build",
    "beforeDevCommand": "pnpm run dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1425"
  },
  "bundle": {
    "active": false,
    "icon": ["icons/icon.ico"]
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Playground",
        "width": 800,
        "height": 600,
        "visible": false,
        "fullscreen": false,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {}
}
