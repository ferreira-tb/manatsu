{
  "name": "@manatsu/vue-plugin",
  "description": "Vue plugin for Manatsu",
  "version": "0.5.1",
  "license": "MIT",
  "private": false,
  "packageManager": "pnpm@8.15.4",
  "homepage": "https://github.com/tsukilabs/manatsu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsukilabs/manatsu.git"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "manatsu",
    "vue"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "pnpm run \"/^(type-check|build-only)$/\"",
    "build-only": "vite build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@manatsu/shared": "workspace:*",
    "@tb-dev/utils": "^1.3.1",
    "@tb-dev/utility-types": "^1.2.0"
  },
  "devDependencies": {
    "vite": "^5.1.6"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.9.0",
    "vue": "^3.4.21"
  },
  "engines": {
    "node": ">=20"
  }
}
