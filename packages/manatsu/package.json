{
  "name": "manatsu",
  "version": "0.1.11",
  "description": "Component library",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/ferreira-tb/manatsu",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferreira-tb/manatsu.git"
  },
  "keywords": [
    "component library",
    "vue"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./style": "./dist/style.css",
    "./themes/manatsu-dark": "./dist/themes/manatsu-dark.css",
    "./themes/manatsu-light": "./dist/themes/manatsu-light.css"
  },
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write"
  },
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@manatsu/components": "workspace:*"
  },
  "engines": {
    "node": ">=18"
  }
}
