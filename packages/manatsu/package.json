{
  "name": "manatsu",
  "version": "0.1.15",
  "description": "Component library",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/ferreira-tb/manatsu",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferreira-tb/manatsu.git"
  },
  "keywords": [
    "component library",
    "vue"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*",
    "./style": "./dist/style.css"
  },
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@manatsu/components": "workspace:*",
    "@manatsu/composables": "workspace:*"
  },
  "peerDependencies": {
    "@manatsu/components": "latest",
    "@manatsu/composables": "latest",
    "@vueuse/core": "^10.0.0",
    "vue": "^3.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
