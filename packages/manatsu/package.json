{
  "name": "manatsu",
  "version": "0.1.47",
  "description": "Vue component library for Tauri apps",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/manatsujs/manatsu",
  "repository": {
    "type": "git",
    "url": "https://github.com/manatsujs/manatsu.git"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "manatsu",
    "component library",
    "vue"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
