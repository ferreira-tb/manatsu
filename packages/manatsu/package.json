{
  "name": "manatsu",
  "version": "0.4.1",
  "description": "Vue component library for Tauri apps",
  "license": "MIT",
  "private": false,
  "packageManager": "pnpm@8.15.0",
  "homepage": "https://github.com/tsukilabs/manatsu",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsukilabs/manatsu.git"
  },
  "bugs": {
    "url": "https://github.com/tsukilabs/manatsu/issues"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "manatsu",
    "components",
    "composables",
    "vue",
    "tauri"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*",
    "./style": "./dist/style.css"
  },
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@manatsu/components": "workspace:*",
    "@manatsu/composables": "workspace:*",
    "@manatsu/shared": "workspace:*",
    "@tb-dev/utility-types": "^1.1.0",
    "@tb-dev/utils": "^1.2.1"
  },
  "devDependencies": {
    "@manatsu/sass": "workspace:*",
    "vite": "^5.0.12"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^1.5.0",
    "@vueuse/core": "^10.7.2",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "engines": {
    "node": ">=20"
  }
}
