{
  "name": "manatsu",
  "version": "0.4.5",
  "description": "Vue component library for Tauri apps",
  "license": "MIT",
  "private": false,
  "packageManager": "pnpm@8.15.4",
  "homepage": "https://github.com/tsukilabs/manatsu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsukilabs/manatsu.git"
  },
  "bugs": {
    "url": "https://github.com/tsukilabs/manatsu/issues"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "manatsu",
    "components",
    "composables",
    "vue",
    "tauri"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*",
    "./style": "./dist/style.css"
  },
  "scripts": {
    "build": "pnpm run \"/^(type-check|build-only)$/\"",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@manatsu/components": "workspace:*",
    "@manatsu/composables": "workspace:*",
    "@manatsu/shared": "workspace:*",
    "@tb-dev/utility-types": "^1.1.7",
    "@tb-dev/utils": "^1.2.9"
  },
  "devDependencies": {
    "@manatsu/sass": "workspace:*",
    "vite": "^5.1.4"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^1.5.3",
    "@vueuse/core": "^10.9.0",
    "vue": "^3.4.20",
    "vue-router": "^4.3.0"
  },
  "engines": {
    "node": ">=20"
  }
}
