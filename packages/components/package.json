  },
  "devDependencies": {
    "@manatsu/sass": "workspace:*",
    "vite": "^5.0.11"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^1.5.0",
    "@vueuse/core": "^10.7.2",
