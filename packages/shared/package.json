{
  "name": "@manatsu/shared",
  "description": "Utilities for Manatsu",
  "version": "1.0.3",
  "license": "MIT",
  "private": false,
  "packageManager": "pnpm@9.7.0",
  "homepage": "https://github.com/ferreira-tb/manatsu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreira-tb/manatsu.git"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "manatsu"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
