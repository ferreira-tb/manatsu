  "dependencies": {
    "@tb-dev/utils": "^2.1.0"
  },
  "devDependencies": {
    "vite": "^5.4.0"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.11.1",
    "vue": "^3.4.37"
  },
  "engines": {
    "node": ">=20"
  }
}