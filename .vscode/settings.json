{
    "vsicons.associations.files": [
        {
            "icon": "tsconfig",
            "extensions": ["tsconfig.rollup.json"],
            "filename": true,
            "format": "svg"
        }
    ],
    "prettier.requireConfig": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
