{
  "vsicons.associations.files": [
    {
      "icon": "tailwind",
      "extensions": ["tailwind.config.mjs"],
      "filename": true,
      "format": "svg"
    }
  ],
  "vsicons.associations.folders": [
    { "icon": "theme", "extensions": ["presets"], "format": "svg" }
  ],
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "files.exclude": {
    ".eslintcache": true
  }
}
