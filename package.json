{
  "name": "@manatsu/monorepo",
  "version": "0.2.21",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,?(s)css,vue,md,json,yaml}": "prettier --write"
  },
  "scripts": {
    "build": "pnpm run cli dev build",
    "build:shared": "pnpm run -F @manatsu/shared build",
    "bump": "miho bump --no-ask --add .",
    "cli": "cargo run -p manatsu --",
    "clippy": "run-s clippy:*",
    "clippy:manatsu": "cargo clippy -p manatsu -- -D warnings",
    "clippy:playground": "cargo clippy --manifest-path=packages/playground/src-tauri/Cargo.toml -- -D warnings",
    "clippy:plugin-manatsu": "cargo clippy -p tauri-plugin-manatsu -- -D warnings",
    "component": "pnpm run cli dev component",
    "composable": "pnpm run cli dev composable",
    "dev": "cargo tauri dev",
    "docs:cli": "cargo doc --open -p manatsu",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "run-p lint:*",
    "lint:style": "stylelint **/*.{scss,vue} --cache",
    "lint:ts": "eslint . --config eslint.config.js --cache",
    "prepare": "husky install",
    "release": "pnpm run build && pnpm run cli dev release",
    "test": "run-p test:*",
    "test:cli": "cargo test -p manatsu",
    "test:packages": "vitest --run",
    "type-check": "vue-tsc --noEmit",
    "ui": "vitest --ui --watch"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@tauri-apps/api": "^1.5.3",
    "@tb-dev/eslint-config": "^1.6.2",
    "@tb-dev/stylelint-config": "^1.1.3",
    "@tb-dev/utility-types": "^1.0.10",
    "@tb-dev/utils": "^1.1.4",
    "@types/node": "^20.11.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/browser": "1.2.0",
    "@vitest/coverage-istanbul": "1.2.0",
    "@vitest/ui": "1.2.0",
    "@vue/test-utils": "^2.4.3",
    "@vueuse/core": "^10.7.2",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.2.2",
    "prettier-plugin-css-order": "^2.0.1",
    "rollup": "^4.9.5",
    "sass": "^1.69.7",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "taze": "^0.13.1",
    "tslib": "^2.6.2",
    "typescript": "5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.1",
    "vite-plugin-vue-devtools": "^7.0.10",
    "vitest": "1.2.0",
    "vue": "~3.3.13",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27",
    "webdriverio": "^8.27.2"
  }
}
