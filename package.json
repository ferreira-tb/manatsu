{
  "name": "@manatsu/monorepo",
  "version": "0.9.5",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.6",
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,?(s)css,vue,md,json,yaml}": "prettier --write"
  },
  "scripts": {
    "build": "mana-dev build",
    "build:shared": "mana-dev build -p shared",
    "clippy": "pnpm run \"/^clippy:.*/\"",
    "clippy:manatsu": "cargo clippy -p manatsu -- -D warnings",
    "clippy:manatsu-dev": "cargo clippy -p manatsu-dev -- -D warnings",
    "clippy:plugin-manatsu": "cargo clippy -p tauri-plugin-manatsu -- -D warnings",
    "docs": "cargo doc --open -p manatsu",
    "format": "prettier . --write && cargo fmt --all",
    "format-check": "prettier . --check",
    "install-dev": "cargo install --path crates/manatsu-dev --force",
    "lint": "eslint . --config eslint.config.js --cache",
    "pedantic": "cargo clippy -- -W clippy::pedantic",
    "prepare": "husky",
    "release": "pnpm run \"/^(test|clippy)$/\" && pnpm run build && mana-dev release",
    "release:crate": "pnpm run \"/^(test:crate|clippy)$/\" && pnpm run build && mana-dev release --only-crate",
    "release:package": "pnpm run test:package && pnpm run build && mana-dev release --only-package",
    "test": "vitest --run",
    "type-check": "pnpm run -r --bail --parallel --aggregate-output type-check",
    "ui": "vitest --ui --watch --no-open --silent",
    "update": "miho update major -t"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.0.0-beta.7",
    "@tb-dev/eslint-config": "^3.4.0",
    "@tb-dev/utility-types": "^1.2.5",
    "@tb-dev/utils": "^1.5.0",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.4.0",
    "@vueuse/core": "^10.9.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.1.2",
    "sass": "^1.74.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.11"
  }
}
