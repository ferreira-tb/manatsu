{
  "name": "@manatsu/monorepo",
  "version": "0.1.12",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write",
    "*.{?(c|m)@(j|t)s,vue}": "pnpm lint"
  },
  "scripts": {
    "build": "miho -l build",
    "build:playground": "pnpm -F @manatsu/playground build",
    "deps": "pnpm exec taze -f -r -w -l",
    "dev": "vite serve packages/playground",
    "prepare": "husky install",
    "format": "pnpm exec prettier . --write",
    "format-check": "pnpm exec prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "release": "miho -l publish",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui --watch --coverage.enabled=true",
    "test:watch": "vitest --watch",
    "type-check": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@manatsu/theme-builder": "^0.2.0",
    "@octokit/core": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.5",
    "@tb-dev/eslint-config": "^1.4.4",
    "@tb-dev/utility-types": "^1.0.0",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/browser": "1.0.2",
    "@vitest/coverage-istanbul": "1.0.2",
    "@vitest/ui": "1.0.2",
    "@vue/test-utils": "^2.4.3",
    "@vueuse/core": "^10.7.0",
    "eslint": "^8.55.0",
    "execa": "^8.0.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "miho": "^2.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.0",
    "prettier-plugin-css-order": "^2.0.1",
    "rollup": "^4.7.0",
    "sass": "^1.69.5",
    "taze": "^0.13.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "1.0.2",
    "vue": "^3.3.11",
    "vue-tsc": "^1.8.25",
    "webdriverio": "^8.24.12"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  }
}
